<%= form_for(@festival, url: { action: "update", method: "PATCH" }) do |f| %>
  <%= f.label(:title) %>
  <%= f.text_field(:title) %>

  <%= f.label(:start_time) %>
  <%= f.datetime_select(:start_time) %>

  <%= f.label(:end_time) %>
  <%= f.datetime_select(:end_time) %>

  <%= f.label(:location) %>
  <%= f.text_field(:location) %>

  <%= f.label(:website_url) %>
  <%= f.text_field(:website_url) %>

  <%= f.submit "Modify Festival" %>
<% end %>